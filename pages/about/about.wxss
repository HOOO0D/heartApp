.container {
  display: flex;
  flex-direction: column;
  height: 100vh;
  padding: 16rpx;
  background-color: #f9f9f9;
}

.toolbar {
  margin-bottom: 16rpx;
  display: flex;
  justify-content: flex-end;
}

.clear-btn {
  padding: 10rpx 20rpx;
  background-color: #ff4d4f;
  color: #fff;
  border-radius: 10rpx;
  font-size: 28rpx;
}

.data-list {
  flex: 1;
  overflow: hidden;
}

.data-item {
  padding: 12rpx;
  margin-bottom: 8rpx;
  background-color: #ffffff;
  border-radius: 8rpx;
  box-shadow: 0 2rpx 6rpx rgba(0, 0, 0, 0.1);
}

.timestamp {
  font-size: 26rpx;
  color: #888;
  margin-bottom: 8rpx;
}

.values text {
  margin-right: 16rpx;
  font-size: 30rpx;
  color: #333;
}
